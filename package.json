{
  "name": "TheChatWeb",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/img/mac/Logo1024x1024.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . TheChatWeb --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/img/win/Logo.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"TheChatWeb\"",    
    "package-linux": "electron-packager . TheChatWeb --overwrite --asar --platform=linux --arch=x64 --icon=assets/img/linux/Logo.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "PixelOrange",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {}
}
